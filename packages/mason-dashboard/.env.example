# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Supabase Service Role Key (server-side only, for elevated operations)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Anthropic API Key (for PRD generation)
ANTHROPIC_API_KEY=your-anthropic-api-key

# NextAuth Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-nextauth-secret-generate-with-openssl-rand-base64-32

# GitHub OAuth App (Settings > Developer Settings > OAuth Apps)
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# Optional: Dashboard URL (for links in CLI output)
NEXT_PUBLIC_DASHBOARD_URL=http://localhost:3000
